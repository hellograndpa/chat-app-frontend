{
  "name": "starterkit-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "mapbox-gl": "^1.5.0",
    "node-sass": "^4.13.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-images-upload": "^1.2.7",
    "react-map-gl": "^5.1.1",
    "react-map-gl-geocoder": "^2.0.11",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "reset-css": "^5.0.1",
    "socket.io-client": "^2.3.0",
    "viewport-mercator-project": "^6.2.2"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  }
}
